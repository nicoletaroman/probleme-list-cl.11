pr=1
s=0
suma=0
x=[3,2,5,6,8]
y=x
for i in range(0,3):
    s=s+x[i]
print("suma primilor 3 termeni este",s)
for e in range(0,len(y)):
    suma=suma+y[e]
print("suma termenilor din lista y este",suma)
for u in range(0,len(x)):
    pr=pr*x[u]
print("produsul termenilor din lista x este",pr)
print("valoarea absoluta a termenului nr 3 din lista x este",abs(x[2]))
print("suma primilor termeni este",x[0]+y[0])